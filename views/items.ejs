<% let bodyParts = ['<h1 class="title">All Items</h1><ul class="item-list">']; %>
<% items.forEach(function(item) { %> 
    <% bodyParts.push(`
        <li class="item">
            <p class="item-name">${item.name}</p>
            <p class="item-description">${item.description}</p>
            <p class="item-price">Â£${item.price}</p>
            <p class="item-qty">Qty: ${item.qty}</p>
        </li>
    `); %>
<% });%> 
<% bodyParts.push('</ul>'); %> 
<% let body = bodyParts.join(''); %>
<%- include('layout', { title: 'Items', body: body }) %>